{
    "scopeName": "master-styles",
    "name": "source.master-styles",
    "injectionSelector": "L:source - (meta.embedded), L:text - (mate.embedded)",
    "patterns": [
        {
            "include": "#highlight-class"
        },
        {
            "include": "#highlight-tsx-class"
        }
    ],
    "repository": {
        "highlight-class": {
            "begin": "(?:\\s*)?(className|class)(\\=)(\")",
            "end": "(\")",
            "captures": {
                "1": {
                    "name": "entity.other.attribute-name"
                },
                "2": {
                    "name": "punctuation.separator.key-value"
                },
                "3": {
                    "name": "string.quoted.double.html"
                }
            },
            "endCaptures": {
                "1": {
                    "name": "string.quoted.double.html"
                }
            },
            "contentName": "meta.embedded.block.master-styles.class",
            "patterns": [
                {
                    "include": "#highlight-styles-key"
                },
                {
                    "include": "#highlight-styles-selector"
                },
                {
                    "include": "#highlight-styles-value"
                },
                {
                    "include": "#highlight-styles-media"
                },
                {
                    "include": "#highlight-class-string"
                }
            ]
        },
        "highlight-tsx-class": {
            "begin": "(?:\\s*)?(className|class)(\\=)(\\s?\\{)",
            "end": "(\\})",
            "captures": {
                "1": {
                    "name": "entity.other.attribute-name"
                },
                "2": {
                    "name": "keyword.operator.assignment.tsx"
                },
                "3": {
                    "name": "punctuation.section.embedded.begin.tsx"
                }
            },
            "endCaptures": {
                "1": {
                    "name": "punctuation.section.embedded.end.tsx"
                }
            },
            "contentName": "meta.embedded.block.master-styles.class",
            "patterns": [
                {
                    "begin": "\\s?(\\w*)(`)",
                    "end": "`",
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.string.template.begin.tsx"
                        },
                        "2": {
                            "name": "string.quoted.double.html"
                        }
                    },
                    "endCaptures": {
                        "1": {
                            "name": "string.quoted.double.html"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#highlight-styles-key"
                        },
                        {
                            "include": "#highlight-styles-selector"
                        },
                        {
                            "include": "#highlight-styles-value"
                        },
                        {
                            "include": "#highlight-styles-media"
                        },
                        {
                            "include": "#highlight-class-string"
                        }
                    ]
                },
                {
                    "begin": "\"",
                    "end": "\"",
                    "captures": {
                        "1": {
                            "name": "string.quoted.double.html"
                        }
                    },
                    "endCaptures": {
                        "1": {
                            "name": "string.quoted.double.html"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#highlight-styles-key"
                        },
                        {
                            "include": "#highlight-styles-selector"
                        },
                        {
                            "include": "#highlight-styles-value"
                        },
                        {
                            "include": "#highlight-styles-media"
                        },
                        {
                            "include": "#highlight-class-string"
                        }
                    ]
                }
            ]
        },
        "highlight-styles-key": {
            "patterns": [
                {
                    "match": "(?<![^\\s\"`]+)(@|~)",
                    "captures": {
                        "1": {
                            "name": "support.type.property-name.css"
                        }
                    }
                },
                {
                    "match": "(?!static|fixed|sticky|hidden|hide|block|table|contents|inline\\-block|inline\\-flex|inline\\-grid|inline\\-table|inline|rounded|round|flex|square)(?<![^\\s:]+:[^\\s]*)([^\\s:]+)(?<!\\([^\\)]*\\))(:)",
                    "captures": {
                        "1": {
                            "name": "support.type.property-name.css"
                        },
                        "2":{
                            "name": "punctuation.separator.key-value.css"
                        }
                    }
                }
            ]
        },
        "highlight-styles-value": {
            "patterns": [
                {
                    "match": "(?<=[^\\s:]+:)(var)(\\()(\\-\\-\\S*)(\\))",
                    "captures": {
                        "1": {
                            "name": "support.function.misc.css"
                        },
                        "2": {
                            "name": "punctuation.section.function.begin.bracket.round.css"
                        },
                        "3": {
                            "name": "variable.argument.css"
                        },
                        "4": {
                            "name": "punctuation.section.function.end.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "(?<=[^\\s:]+:)(\\$)(\\()(\\S*)(\\))",
                    "captures": {
                        "1": {
                            "name": "support.function.misc.css"
                        },
                        "2": {
                            "name": "punctuation.section.function.begin.bracket.round.css"
                        },
                        "3": {
                            "name": "variable.argument.css"
                        },
                        "4": {
                            "name": "punctuation.section.function.end.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "([^\\s\\:;,@]+)\\(",
                    "captures": {
                        "1": {
                            "name": "support.function.misc.css"
                        },
                        "2": {
                            "name": "punctuation.section.function.begin.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "(static|fixed|sticky|hidden|hide|block|table|contents|inline\\-block|inline\\-flex|inline\\-grid|inline\\-table|inline|rounded|round)",
                    "captures": {
                        "1": {
                            "name": "support.constant.property-value.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"]*)(\\+|\\-|\\*|\\/)?(\\d+\\.\\d+|\\.\\d+|\\d+)(\\%|ch|cm|em|ex|in|mm|pc|pt|px|rem|vh|vmax|vmin|vw|deg|grad|rad|turn|s)?",
                    "captures": {
                        "1": {
                            "name": "constant.numeric.css"
                        },
                        "2": {
                            "name": "constant.numeric.css"
                        },
                        "3": {
                            "name": "keyword.other.unit"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"\\:]*)([^\\s\"`:@'_!\\[;,\\(\\)]+)",
                    "captures": {
                        "1": {
                            "name": "support.constant.property-value.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"\\:]+)(;)",
                    "captures": {
                        "1": {
                            "name": "punctuation.terminator.rule.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"\\:]+)(,)",
                    "captures": {
                        "1": {
                            "name": "punctuation.separator.list.comma.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"\\:]+)(!)",
                    "captures": {
                        "1": {
                            "name": "keyword.other.important.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"\\:]+)(\\()",
                    "captures": {
                        "1": {
                            "name": "punctuation.section.function.begin.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:(?:[^\\s:]+:)|@|~|(?:[^\\s\\:;,]+\\())[^\\s\"\\:]+)(\\))",
                    "captures": {
                        "1": {
                            "name": "punctuation.section.function.end.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "(?<=[^\\s:]+:)(?:('[^'\\s]*')|([^\\s\"`:@'_!\\[]+))(?:(?<![;/\\(\\,])\\.(?!\\d))?(!)?",
                    "captures": {
                        "1": {
                            "name": "string.quoted.double.html"
                        },
                        "2": {
                            "name": "support.constant.property-value.css"
                        },
                        "3": {
                            "name": "keyword.other.important.css"
                        }
                    }
                }
            ]
        },
        "highlight-styles-selector": {
            "patterns": [
                {
                    "match": "(?<![;/\\,])(\\.)(?!\\d)([^\\s\"`@:><.\\[\\]_\\+\\(\\)]*)",
                    "captures": {
                        "1": {
                            "name": "entity.other.attribute-name.class.css"
                        },
                        "2": {
                            "name": "entity.other.attribute-name.class.css"
                        }
                    }
                },
                {
                    "match": "(?:(\\:{1,2}|<|_|\\@)|(\\+(?!\\d)|>|~))(lang|nth\\-child|nth\\-last\\-child|nth\\-of\\-type|nth\\-last\\-of\\-type|host\\-context|host|is|not|where)(\\()",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.combinator.css"
                        },
                        "2": {
                            "name": ""
                        },
                        "3": {
                            "name": "entity.other.attribute-name.pseudo-class.css"
                        },
                        "4": {
                            "name": "punctuation.section.function.begin.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "(?<=(?:\\:{1,2}|>|<|_|\\@|\\+|~)(?:lang|nth\\-child|nth\\-last\\-child|nth\\-of\\-type|nth\\-last\\-of\\-type|host\\-context|host|is|not|where)\\([^\\s\\(]*)(\\))",
                    "captures": {
                        "1": {
                            "name": "punctuation.section.function.end.bracket.round.css"
                        }
                    }
                },
                {
                    "match": "(?:(\\:{1,2}|<|_|\\@)|(\\+(?!\\d)|>|~)|(\\[)|(\\]))(?:(any\\-link|visited|target|scope|hover|active|focus\\-visible|focus\\-within|focus|autofill|enabled|disabled|read\\-only|read\\-write|placeholder\\-showen|default|checked|indeterminate|valid|invalid|in\\-range|out\\-of\\-range|required|optional|root|empty|first\\-letter|first\\-child|first\\-of\\-type|last\\-child|last\\-of\\-type|only\\-child|only\\-of\\-type|defined|first|fullscreen|left)|([^\\s\"`@:><.\\[\\]_\\+\\(\\)]*))",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.combinator.css"
                        },
                        "2": {
                            "name": ""
                        },
                        "3": {
                            "name": "punctuation.definition.entity.begin.bracket.square.css"
                        },
                        "4": {
                            "name": "punctuation.definition.entity.end.bracket.square.css"
                        },
                        "5": {
                            "name": "entity.other.attribute-name.pseudo-class.css"
                        },
                        "6": {
                            "name": "entity.name.tag.css"
                        }
                    }
                }
            ]
        },
        "highlight-styles-media": {
            "patterns": [
                {
                    "match": "(?<=[^\\s:]+:(?:(?:[^\\s@]*)|(?:'[^\\s]*'[^\\s@]*)))(@[^\\s\"`']+)",
                    "captures": {
                        "1": {
                            "name": "constant.character.escape"
                        }
                    }
                }
            ]
        },
        "highlight-class-string": {
            "patterns": [
                {
                    "match": "([^\\s])",
                    "captures": {
                        "1": {
                            
                        }
                    }
                }
            ]
        }
    }
}